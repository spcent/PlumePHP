{
    "name": "spcent/plumephp",
    "type": "library",
    "description": "一个单文件极简的php框架，可运行在fpm或者swoole协程模式下",
    "keywords": [
      "swoole",
      "framework",
      "async",
      "plumephp"
    ],
    "license": "Apache-2.0",
    "authors": [
      {
        "name": "spcent",
        "email": "spcent@qq.com"
      }
    ],
    "require": {
      "php": ">=7.1"
    },
    "autoload": {
      "psr-4": {
        "Plume\\": "src/"
      }
    },
    "suggest": {
      "ext-redis": "如果缓存驱动为Redis",
      "ext-msgpack": "globalData和Rpc服务通讯的打包",
      "ext-swoole": "常驻内存运行必须的扩展",
      "ext-pdo": "数据库驱动"
    }
  }